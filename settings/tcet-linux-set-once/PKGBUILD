# Maintainer: Akash6222
# Project: TCET Linux

pkgname=tcet-linux-set-once
pkgver=23.08
pkgrel=1
pkgdesc="Set Once for TCET Linux"
arch=('x86_64')
url="https://github.com/harshau007/testing-repo/releases/download"
license=('GPL3')

conflicts=()
provides=("${pkgname}")
options=(!strip !emptydirs)
source=("$pkgname.tar.gz::$url/2023.09.07/$pkgname.tar.gz")
sha256sums=('b5351f06d996e66a94378aaf8935aac4fb6b73fa347ff1e261e7321c52c9fba3')

package() {

    install -dm755 ${pkgdir}/
    cp -r ${srcdir}/${pkgname}/* ${pkgdir}/

}
