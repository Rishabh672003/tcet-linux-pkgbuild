# Maintainer: Akash6222
# Project: TCET Linux

pkgname=tcet-linux-wallpaper
pkgver=23.08
pkgrel=4
pkgdesc="Wallpaper for TCET Linux"
arch=('x86_64')
url="https://github.com/harshau007/testing-repo/releases/download"
license=('GPL3')

conflicts=()
provides=("${pkgname}")
options=(!strip !emptydirs)
source=("$pkgname.tar.gz::$url/2023.09.07/$pkgname.tar.gz")
sha256sums=('1f091ea4fefa055367dbb8ae055601b766e1b8765ce1591bc167e7043a658b2f')

destname="/usr/share/backgrounds/xfce/"

package() {

    install -dm755 ${pkgdir}${destname}/
    cp -r ${srcdir}/${pkgname}/* ${pkgdir}/${destname}/

}
